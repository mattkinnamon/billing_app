<%= form_for @company do |f| %>

<%= f.label :company_name, 'Company Name' %><br>
<%= f.text_field :company_name %><br>
<br>
<%= f.label :contact %><br>
<%= f.text_field :contact %><br>
<br>
<%= f.label :domain %><br>
<%= f.text_field :domain %><br>
<br>
<%= f.label :phone %><br>
<%= f.text_field :phone %><br>
<br>
<%= f.label :email1, 'Email' %><br>
<%= f.text_field :email1 %><br>
<br>
<%= f.label :street_address, 'Street Address' %><br>
<%= f.text_field :street_address %><br>
<br>
<%= f.label :city %><br>
<%= f.text_field :city %><br>
<br>
<%= f.label :state %><br>
<%= f.text_field :state %><br>
<br>
<%= f.label :zip %><br>
<%= f.text_field :zip %><br>
<br>
<%= f.label :taxable %><br>
<%= f.select :taxable, [['Yes', true], ['No', false]] %><br>
<br>
<%= f.label :billing_rate, 'Billing Rate' %><br>
<%= f.select :billing_rate, [['Standard', 'standard'], ['Custom', 'custom']] %><br><br>
<%= f.label :terms %><br>
<%= f.select :terms, [['Net 30', 30], ['Net 60', 60], ['Net 90', 90]] %><br>
<br>
<%= f.label :purchase_order %><br>
<%= f.text_field :purchase_order %><br>
<br>

<%= f.submit %>
<% end %>